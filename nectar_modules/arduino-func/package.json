    "nectarjs",
    "arduino",
    "iot"
  ],
  "license": "AGPL",
  "licenses": [
    {
      "type": "AGPL",
      "url": "https://github.com/nectarjs/nectarjs/blob/master/LICENSE"
    }
  ],
  "repository":
  {
    "type": "git",
    "url": "git+ssh://git@github.com/NectarJS/nectarjs.git"
  },
  "version": "0.0.1",
  "nectar":
  {
    "env": ["arduino"],
    "target": ["uno", "mega", "mega2560", "mega1280", "nano", "nano2", "nano3"],
